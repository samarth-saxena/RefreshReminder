{
  "name": "electron-app-demo",
  "version": "1.0.0",
  "description": "simple system monitor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=img/logo.jpg --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Sys Info App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=img/logo.jpg --prune=true --out=release-builds"
  },
  "author": "Piyush Sharma",
  "license": "ISC",
  "dependencies": {
    "electron": "^12.0.2"
  },
  "devDependencies": {
    "electron-packager": "^15.2.0"
  }
}
